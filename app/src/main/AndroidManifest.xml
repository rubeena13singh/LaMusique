<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.lamusique">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".nowPlaying"
            android:label="@string/text1" />
        <activity
            android:name=".songDetail"
            android:label="@string/text2" />
        <activity
            android:name=".musicLibrary"
            android:label="@string/text3" />
        <activity
            android:name=".playLists"
            android:label="@string/text4" />
        <activity android:name=".payment"></activity>
    </application>

</manifest>